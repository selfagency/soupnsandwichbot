[Unit]
Description=Soup N' Sandwich Bot

[Service]
ExecStart=/home/selfagency/webapps/bots/soupnsandwichbot/dist/index.js
Restart=always
User=selfagency
# Note Debian/Ubuntu uses 'nogroup', RHEL/Fedora uses 'nobody'
Group=selfagency
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/selfagency/webapps/bots/soupnsandwichbot

[Install]
WantedBy=multi-user.target
